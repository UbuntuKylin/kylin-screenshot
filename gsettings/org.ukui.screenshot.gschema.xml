<schemalist>
    <schema id="org.ukui.screenshot" path="/org/ukui/screenshot/" gettext-domain="screenshot">
    <key type="s" name="screenshot">
            <default>"true"</default>
            <summary>Enable to use kylin-screenshot</summary>
            <description>Whether enable to use kylin-screenshot</description>
    </key>
    <key type="s" name="screenshot-path">
            <default>''</default>
            <summary>kylin-screenshot save path</summary>
            <description>Record  kylin-screenshot save path</description>
    </key>
    <key type="s" name="screenshot-name">
            <default>''</default>
            <summary>kylin-screenshot save name</summary>
            <description>Record  kylin-screenshot saved name</description>
    </key>
    <key type="s" name="screenshot-type">
            <default>".png"</default>
            <summary>kylin-screenshot save type</summary>
            <description>Record  kylin-screenshot save type</description>
    </key>
    <key type="b" name="isrunning">
            <default>false</default>
            <summary>Whether kylin-screenshot is running</summary>
            <description>Whether running kylin-screenshot</description>
    </key>
    </schema>
</schemalist>

